{
  "name": "eregistry-desktop",
  "version": "1.5.0",
  "author": "Yuan <zhencai.yuan@datarx.cn>",
  "description": "e registry desktop",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "electron": "electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web BASE_API='' node --max_old_space_size=4096 .electron-vue/build.js",
    "release": "node .electron-vue/build.js && electron-builder --publish always",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix && electron-builder install-app-deps",
    "patch": "npm version patch && git push origin master && git push origin --tags",
    "minor": "npm version minor && git push origin master && git push origin --tags",
    "major": "npm version major && git push origin master && git push origin --tags"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "YuanZhencai",
        "repo": "eRegistry-desktop",
        "releaseType": "draft"
      }
    ],
    "productName": "患者登记云随访系统",
    "artifactName": "eregistry-desktop-${version}.${ext}",
    "appId": "cn.datarx.eregistry-desktop",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "hardenedRuntime": false,
      "target": "dmg"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "ace-builds": "^1.4.8",
    "agora-electron-sdk": "3.0.0",
    "ant-design-vue": "1.4.12",
    "axios": "^0.17.1",
    "bootstrap": "^3.4.1",
    "distpicker": "2.0.2",
    "echarts": "^4.6.0",
    "electron-dl": "1.12.0",
    "element-ui": "^2.13.0",
    "file-saver": "^2.0.2",
    "jquery": "^3.4.1",
    "js-base64": "^2.5.2",
    "js-cookie": "^2.2.0",
    "js-yaml": "^3.13.1",
    "moment": "^2.24.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "signature_pad": "^2.3.2",
    "sockjs-client": "^1.4.0",
    "sortablejs": "1.6.1",
    "stompjs": "^2.3.3",
    "survey-core": "1.5.12",
    "survey-creator": "1.5.12",
    "survey-knockout": "1.5.12",
    "survey-pdf": "1.5.18",
    "survey-vue": "1.5.12",
    "surveyjs-widgets": "1.5.12",
    "unused-filename": "^2.1.0",
    "vue": "2.5.10",
    "vue-axios": "^2.1.5",
    "vue-count-to": "1.0.13",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.15.5",
    "vue-router": "3.0.1",
    "vuex": "3.0.1",
    "weixin-js-sdk": "^1.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "6.1.12",
    "electron-builder": "21.2.0",
    "electron-debug": "^1.4.0",
    "electron-reload": "^1.0.2",
    "eslint": "^4.4.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "sass-loader": "6.0.6",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^0.18.2",
    "svg-sprite-loader": "3.5.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "2.5.10",
    "webpack": "3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "agora_electron": {
    "electron_version": "5.0.8",
    "prebuilt": true
  },
  "__npminstall_done": false
}
